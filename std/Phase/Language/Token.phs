package Phase::Language {

  class Token {

    type: String
    lexeme: String
    literal
    pos: Position

    new(
      type: String,
      lexeme: String,
      literal = null,
      pos: Position
    ) {
      this.type = type
      this.lexeme = lexeme
      this.literal = literal
      this.pos = pos
    }

  }

}
