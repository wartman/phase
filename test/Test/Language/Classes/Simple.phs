package Test::Language::Classes {

  class Simple {

    static bin = 'bin'

    foo
    private bar

    new(foo) {
      this.foo = foo
    }

    getBar() {
      return this.bar
    }

  }

  ::Expect.that(Simple.bin, 'bin')

  var cls = Simple('foo')

  ::Expect.that(cls.foo, 'foo')
  ::Expect.that(cls.getBar(), 'bar')

}
